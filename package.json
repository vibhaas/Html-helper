{
  "name": "html-helper",
  "version": "2.4.0",
  "description": "HTML Helper is a simple but powerful code editor created with the aim of helping people write HTML code(especially in my school)",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . html-helper --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Html Helper\"",
    "package-linux": "electron-packager . html-helper --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vibhaas/Html-helper.git"
  },
  "keywords": [
    "html",
    "js",
    "css",
    "electron",
    "editor"
  ],
  "author": "Vibhaas Srivastava <vibhaas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vibhaas/Html-helper/issues"
  },
  "homepage": "https://github.com/vibhaas/Html-helper#readme",
  "dependencies": {
    "electron": "^4.0.0",
    "electron-packager": "^13.0.1"
  }
}
